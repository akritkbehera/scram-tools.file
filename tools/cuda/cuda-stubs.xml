  <lib name="cuda"/>
  <use name="cuda-interface"/>
  <client>
    <environment name="CUDA_STUBS_BASE" default="@TOOL_ROOT@"/>
    <environment name="LIBDIR"          default="$CUDA_STUBS_BASE/lib64/stubs"/>
  </client>
  <flags SKIP_TOOL_SYMLINKS="1"/>
</tool>
